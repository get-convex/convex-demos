{
  "name": "convex-cli",
  "description": "Command-line interface for Convex cloud backend.",
  "version": "0.0.0",
  "author": "Zerowatt, Inc. <team@zerowatt.io>",
  "bin": {
    "convex": "./bin/run"
  },
  "bugs": "https://github.com/zerowatt-io/grammar/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/node": "^14.17.3",
    "globby": "^10.0.2",
    "snowpack": "^3.8.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/zerowatt-io/grammar",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "convex",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "zerowatt-io/grammar",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf dist && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "chalk": "^2.4.2",
    "tslib": "^1.14.1",
    "zod": "^3.7.3"
  }
}
